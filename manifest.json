{
    "manifest_version": 3,
    "name": "gptName",
    "version": "3.0",
    "description": "name your GPT",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "content_scripts": [
        {
          "js": ["main.js"],
          "matches": ["https://chat.openai.com/*"]
        }
    ]
}